{
  "name": "felixarntz/wp-gdpr-cookie-notice",
  "description": "Simple performant GDPR-compliant cookie consent notice that supports granular cookie control and live preview customization.",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "gdpr",
    "cookie notice",
    "cookie consent",
    "granular cookie control",
    "customizer"
  ],
  "homepage": "https://wordpress.org/plugins/wp-gdpr-cookie-notice/",
  "authors": [
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "homepage": "https://leaves-and-love.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/felixarntz/wp-gdpr-cookie-notice/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "0.14.*",
    "wimg/php-compatibility": "^8"
  },
  "scripts":     {
    "config-phpcs": [
      "vendor/bin/phpcs --config-set installed_paths ../../wimg/php-compatibility,../../wp-coding-standards/wpcs"
    ]
  }
}
